<div class="well">
<%= link_to 'Back to Index', articles_path %>
</div>

<%= form_for :article, url: article_path(@article), method: :put do |f| %>
<p>
	<%= f.label :title %><br/>
	<%= f.text_field :title %>
</p>

<p>
	<%= f.label :text %><br/>
	<%= f.text_area :text %>
</p>

<p>
	<%= f.submit %>
</p>
<% end %>